<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.asuka.lease.web.admin.mapper.RoomLeaseTermMapper">

    <!--    List<LeaseTerm> getLeaseTermList(Long id);-->
    <select id="getLeaseTermList" resultType="org.asuka.lease.model.entity.LeaseTerm">
        select id, month_count, unit
        from lease.lease_term
        where id in (select lease_term_id
                     from lease.room_lease_term
                     where is_deleted = 0
                       and room_id = #{id})
    </select>
</mapper>
